@article{trowbridge2010,
  title = {Relating {{Road Salt}} to {{Exceedances}} of the {{Water Quality Standard}} for {{Chloride}} in {{New Hampshire Streams}}},
  author = {Trowbridge, Philip R. and Kahl, J. Steve and Sassan, Dari A. and Heath, Douglas L. and Walsh, Edward M.},
