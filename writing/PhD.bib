@article{wanek2011,
  title = {Are Vascular Epiphytes Nitrogen or Phosphorus Limited? {{A}} Study of Plant {{15N}} Fractionation and Foliar {{N}}:{{P}} Stoichiometry with the Tank Bromeliad {{Vriesea}} Sanguinolenta},
