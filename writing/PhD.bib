@article{comin1997,
  title = {Nitrogen Removal and Cycling in Restored Wetlands Used as Filters of Nutrients for Agricultural Runoff},
  author = {Com{\'i}n, Francisco A. and Romero, Jose A. and Astorga, Valeria and Garc{\'i}a, Carmen},
  year = {1997},
  volume = {35},
  pages = {255--261},
  doi = {10.1016/S0273-1223(97)00076-0},
  abstract = {Four restored wetlands dominated by Phragmites australis, Typha latifolia and Scirpus lacustris were used to improve the quality of agricultural runoff in the Delta of the Ebro River (NE Spain) in 1993. The wetlands were continuously flooded with water from a ricefield irrigation network during the growing season and received water with between 0\textendash 270 mg m-2d-1 of total nitrogen, 29\textendash 105 mg m-2d-1 of dissolved inorganic nitrogen and 0\textendash 27 mg m-2d-1 of dissolved organic nitrogen. Surface outflows contained between 0\textendash 80 mg m-2d-1 of total nitrogen, 0\textendash 12 mg m-2d-1 of dissolved inorganic nitrogen and 1\textendash 19 mg m-2d-1 of dissolved organic nitrogen. The nitrogen retention efficiency was close to 100\% of the input, except for dissolved organic nitrogen at the end of the growing season. The denitrification rates measured by the acetylene reduction in the sediment ranged between 0 and 3.46 mg N m-2 d-1 and represented between 0 and 12\% of the inflowing dissolved inorganic nitrogen. Emergent macrophytes accumulated between 20 and 100 mg N m-2 d-1, which accounts for between 66 and 100\% of the inflowing dissolved inorganic nitrogen. The wetland sediment accumulated between III and 250 mg N m-2 d-1 during the six month growing season. The removal rate constants calculated according to a first - order plug - flow kinetics, were between 0.01-0.075 m d-1 for total nitrogen and 0.01\textendash 0.3 m d-1 for dissolved inorganic nitrogen. Plant uptake, detritus accumulation and decomposition, and nitrogen recycling in the sediment are major processes for nitrogen retention and recycling in the wetlands. This type of wetlands, restored from ricefields, act as highly efficient water polishing filters for agricultural runoff and, at the same time, can contribute to increase the habitat biodiversity of large areas where rice is cultivated extensively.},
  journal = {Water Science and Technology},
  number = {5}
}

