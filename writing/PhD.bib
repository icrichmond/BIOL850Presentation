  author = {Pataki, Diane E. and Alberti, Marina and Cadenasso, Mary L. and Felson, Alexander J. and McDonnell, Mark J. and Pincetl, Stephanie and Pouyat, Richard V. and Set{\"a}l{\"a}, Heikki and Whitlow, Thomas H.},
  year = {2021},
  month = apr,
  volume = {9},
