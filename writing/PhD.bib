@article{craine2018,
  title = {Isotopic Evidence for Oligotrophication of Terrestrial Ecosystems},
  author = {Craine, Joseph M. and Elmore, Andrew J. and Wang, Lixin and Aranibar, Julieta and Bauters, Marijn and Boeckx, Pascal and Crowley, Brooke E. and Dawes, Melissa A. and Delzon, Sylvain and Fajardo, Alex and Fang, Yunting and Fujiyoshi, Lei and Gray, Alan and Guerrieri, Rossella and Gundale, Michael J. and Hawke, David J. and Hietz, Peter and Jonard, Mathieu and Kearsley, Elizabeth and Kenzo, Tanaka and Makarov, Mikhail and {Mara{\~n}{\'o}n-Jim{\'e}nez}, Sara and McGlynn, Terrence P. and McNeil, Brenden E. and Mosher, Stella G. and Nelson, David M. and Peri, Pablo L. and Roggy, Jean Christophe and {Sanders-DeMott}, Rebecca and Song, Minghua and Szpak, Paul and Templer, Pamela H. and der Colff, Dewidine Van and Werner, Christiane and Xu, Xingliang and Yang, Yang and Yu, Guirui and {Zmudczy{\'n}ska-Skarbek}, Katarzyna},
  year = {2018},
  volume = {2},
  pages = {1735--1735},
  publisher = {{Springer US}},
  issn = {4155901806940},
  doi = {10.1038/s41559-018-0694-0},
  abstract = {Human societies depend on an Earth system that operates within a constrained range of nutrient availability, yet the recent trajectory of terrestrial nitrogen (N) availability is uncertain. Examining patterns of foliar N concentrations and isotope ratios ({$\delta$}15N) from more than 43,000 samples acquired over 37 years, here we show that foliar N concentration declined by 9\% and foliar {$\delta$}15N declined by 0.6\textendash 1.6\textperthousand. Examining patterns across different climate spaces, foliar {$\delta$}15N declined across the entire range of mean annual temperature and mean annual precipitation tested. These results suggest declines in N supply relative to plant demand at the global scale. In all, there are now multiple lines of evidence of declining N availability in many unfertilized terrestrial ecosystems, including declines in {$\delta$}15N of tree rings and leaves from herbarium samples over the past 75\textendash 150 years. These patterns are consistent with the proposed consequences of elevated atmospheric carbon dioxide and longer growing seasons. These declines will limit future terrestrial carbon uptake and increase nutritional stress for herbivores.},
  file = {/home/icrichmond/Zotero/storage/ZKH9X2LZ/s41559-018-0694-0.pdf},
  journal = {Nature Ecology \& Evolution},
  number = {11}
}

