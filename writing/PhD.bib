}

@article{watson2007,
  title = {Vigilance and Fitness in Grey Partridges {{Perdix}} Perdix: {{The}} Effects of Group Size and Foraging-Vigilance Trade-Offs on Predation Mortality},
  author = {Watson, Mark and Aebischer, Nicholas J. and Cresswell, Will},
  year = {2007},
