@article{graves2017,
  title = {Species Richness Alone Does Not Predict Cultural Ecosystem Service Value},
  author = {Graves, Rose A. and Pearson, Scott M. and Turner, Monica G.},
  year = {2017},
  month = apr,
  volume = {114},
  pages = {3774--3779},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1701370114},
  abstract = {Many biodiversity-ecosystem services studies omit cultural ecosystem services (CES) or use species richness as a proxy and assume that more species confer greater CES value. We studied wildflower viewing, a key biodiversity-based CES in amenity-based landscapes, in Southern Appalachian Mountain forests and asked (               i               ) How do aesthetic preferences for wildflower communities vary with components of biodiversity, including species richness?; (               ii               ) How do aesthetic preferences for wildflower communities vary across psychographic groups?; and (               iii               ) How well does species richness perform as an indicator of CES value compared with revealed social preferences for wildflower communities? Public forest visitors (               n               = 293) were surveyed during the summer of 2015 and asked to choose among images of wildflower communities in which flower species richness, flower abundance, species evenness, color diversity, and presence of charismatic species had been digitally manipulated. Aesthetic preferences among images were unrelated to species richness but increased with more abundant flowers, greater species evenness, and greater color diversity. Aesthetic preferences were consistent across psychographic groups and unaffected by knowledge of local flora or value placed on wildflower viewing. When actual wildflower communities (               n               = 54) were ranked based on empirically measured flower species richness or wildflower viewing utility based on multinomial logit models of revealed preferences, rankings were broadly similar. However, designation of hotspots (CES values above the median) based on species richness alone missed 27\% of wildflower viewing utility hotspots. Thus, conservation priorities for sustaining CES should incorporate social preferences and consider multiple dimensions of biodiversity that underpin CES supply.},
  file = {/home/icrichmond/Zotero/storage/9RCKN8B3/s41559-020-01358-z.pdf;/home/icrichmond/Zotero/storage/VD9MBLK9/Graves et al. - 2017 - Species richness alone does not predict cultural e.pdf},
  journal = {Proceedings of the National Academy of Sciences},
  language = {en},
  number = {14}
}

@article{graves2019,
  title = {Effects of Bird Community Dynamics on the Seasonal Distribution of Cultural Ecosystem Services},
