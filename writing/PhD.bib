@article{norton2016,
  title = {Urban {{Biodiversity}} and {{Landscape Ecology}}: {{Patterns}}, {{Processes}} and {{Planning}}},
  author = {Norton, Briony A. and Evans, Karl L. and Warren, Philip H.},
  year = {2016},
