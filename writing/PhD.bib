@article{fourcade,
  title = {Habitat Amount and Distribution Modify Community Dynamics under Climate Change},
  author = {Fourcade, Yoan and WallisDeVries, Michiel F. and Kuussaari, Mikko and van Swaay, Chris A. M. and Heli{\"o}l{\"a}, Janne and {\"O}ckinger, Erik},
  volume = {n/a},
  issn = {1461-0248},
  doi = {10.1111/ele.13691},
  abstract = {Habitat fragmentation may present a major impediment to species range shifts caused by climate change, but how it affects local community dynamics in a changing climate has so far not been adequately investigated empirically. Using long-term monitoring data of butterfly assemblages, we tested the effects of the amount and distribution of semi-natural habitat (SNH), moderated by species traits, on climate-driven species turnover. We found that spatially dispersed SNH favoured the colonisation of warm-adapted and mobile species. In contrast, extinction risk of cold-adapted species increased in dispersed (as opposed to aggregated) habitats and when the amount of SNH was low. Strengthening habitat networks by maintaining or creating stepping-stone patches could thus allow warm-adapted species to expand their range, while increasing the area of natural habitat and its spatial cohesion may be important to aid the local persistence of species threatened by a warming climate.},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13691},
  file = {/home/icrichmond/Zotero/storage/LV9PC7PC/Fourcade et al. - Habitat amount and distribution modify community d.pdf;/home/icrichmond/Zotero/storage/CT4PK2ZI/ele.html},
