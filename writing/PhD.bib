@article{welp2007,
  title = {The Sensitivity of Carbon Fluxes to Spring Warming and Summer Drought Depends on Plant Functional Type in Boreal Forest Ecosystems},
  author = {Welp, L R and Randerson, J T and Liu, H P},
  year = {2007},
  volume = {147},
